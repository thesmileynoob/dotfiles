# Parent editor config with sane defaults
# Change according to specific projects


root = true


# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# 4 space indentation for php
[*.{py, php, }]
indent_style = space
indent_size = 4

[*.{html, css}]
indent_size = 2
